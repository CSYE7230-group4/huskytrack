{
  "name": "huskytrack",
  "version": "1.0.0",
  "description": "HuskyTrack - Campus Life Tracker for Northeastern Students!",
  "main": "index.js",
  "scripts": {
    "dev": "npx concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CSYE7230-group4/huskytrack.git"
  },
